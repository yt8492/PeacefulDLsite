{
  "manifest_version": 3,
  "name": "Peaceful Internet",
  "version": "1.0",
  "description": "Make internet peace again",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "option.html",
  "permissions": [
    "storage"
  ]
}
