{
  "manifest_version": 3,
  "name": "Peaceful Internet",
  "version": "1.0",
  "description": "Make internet peace again",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "run_at":"document_start"
}
